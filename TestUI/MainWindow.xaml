<Window
    x:Class="TestUI.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:TestUI"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:uc="clr-namespace:Revit.Application.Views.FamilyViews;assembly=Revit.Application"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Revit.Application;component/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <ContentControl
            Margin="10"
            Background="LightBlue"
            Style="{StaticResource ContentControl_Normal}">
            <StackPanel>

                <Button
                    Width="100"
                    Height="25"
                    VerticalAlignment="Center"
                    Content="121231233" />
                <uc:FamilyLibrayManagerView />

            </StackPanel>

        </ContentControl>
    </Grid>
</Window>
