<UserControl
    x:Class="Revit.Application.Views.FamilyViews.PublicViews.DialogViews.PublicUploadFamilyDialogView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Revit.Application.Views.FamilyViews.PublicViews.DialogViews"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="400"
    mc:Ignorable="d">
    <Grid>
        <ScrollViewer DataContext="{Binding User}" ScrollViewer.VerticalScrollBarVisibility="Auto">
            <StackPanel Margin="5,0">
                <TextBlock Text="账户" />
                <TextBox Text="{Binding UserName}" />
                <TextBlock Text="用户名" />
                <TextBox Text="{Binding FullName}" />
                <TextBlock Text="密码" />
                <TextBox Text="{Binding Password}" />
                <TextBlock Text="二次确认密码" />
                <TextBox Text="{Binding Password}" />
            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
